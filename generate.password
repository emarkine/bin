require 'rubygems'
require 'rush'

def newpass( len )
    chars = ("a".."z").to_a + ("A".."Z").to_a + 
    1.upto(len) do |i| 
        newpass << chars[rand(chars.size-1)] 
    end	
    number = ("0".."9").to_a
    position = ("0".."9").to_a
    newpass
end

local = Rush::Box.new('localhost')
len = ARGV[0] 
len ||= 10

puts newpass(len.to_i)
 

